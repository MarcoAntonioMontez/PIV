function [ boxes ] = calc_boxes( matrix, num_classes )
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here

boxes=zeros(8,3*num_classes);




for i = 1:num_classes
    j=i-1;
    [x,y,z] = find(matrix==i);
    max_x=max(x);
    max_y=max(y);
    max_z=max(z);
    
    min_x=min(x);
    min_y=min(y);
    min_z=min(z);
    
    boxes(1,:j+2)=()

end

